    <xsd:complexType name="com.ibm.ws.persistence.databaseStore"> 
        <xsd:annotation> 
            <xsd:documentation>Реляционная база данных как постоянное хранилище для компонентов сервера.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Хранилище базы данных</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="dataSource" type="com.ibm.ws.jdbc.dataSource"> 
                <xsd:annotation> 
                    <xsd:documentation>Источник данных, который подключается к постоянному хранилищу.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Источник данных</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="authData" type="com.ibm.ws.security.jca.internal.authdata.config"> 
                <xsd:annotation> 
                    <xsd:documentation>Данные идентификации для планирования, запроса и выполнения задач.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Идентификационные данные</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="createTables" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Если эта опция равна true, то создаются таблицы базы данных.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Создать таблицы базы данных</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="dataSourceRef" use="optional" default="DefaultDataSource" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Источник данных, который подключается к постоянному хранилищу.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ссылка на источник данных</ext:label> 
                    <ext:reference>dataSource</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="keyGenerationStrategy" use="optional" default="AUTO"> 
            <xsd:annotation> 
                <xsd:documentation>Предпочитаемая стратегия создания уникальных первичных ключей. Если выбранная стратегия не поддерживается для базы данных, то можно использовать другую стратегию.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Стратегия создания ключей</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="AUTO"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Автоматически выбирать стратегию генерации уникальных первичных ключей. </xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="IDENTITY"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Использовать для создания уникальных первичных ключей столбец-идентификатор базы данных.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="SEQUENCE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Использовать для создания уникальных первичных ключей последовательность базы данных.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="TABLE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Использовать для создания уникальных первичных ключей таблицу базы данных.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="tablePrefix" use="optional" default="WLP" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Префикс имени для таблиц, последовательностей и других артефактов базы данных.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Префикс имени таблицы</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authDataRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Данные идентификации для планирования, запроса и выполнения задач.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ссылка на идентификационные данные</ext:label> 
                    <ext:reference>authData</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="schema" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Имя схемы с правами на чтение таблиц базы данных и запись в них.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Имя схемы</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
