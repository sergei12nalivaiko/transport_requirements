        <xsd:attribute name="sslRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>ИД конфигурации SSL, применяемой для подключения к социальным медиа.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ссылка на SSL</ext:label> 
                    <ext:reference>ssl</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authFilterRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Задает ссылку на фильтр идентификации.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ссылка на фильтр идентификации</ext:label> 
                    <ext:reference>authFilter</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustAliasName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Псевдоним доверенного ключа, указывающий на открытый ключ, используемый для проверки подписи маркера.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Псевдоним доверенного ключа</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="isClientSideRedirectSupported" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Указывает, поддерживается ли перенаправление на стороне клиента. Клиентом может быть браузер или отдельное приложение JavaScript. Если указано значение Да, клиент должен поддерживать JavaScript.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Поддержка перенаправления на стороне клиента</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="displayName" use="optional" default="oidcLogin" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Отображаемое имя для конфигурации входа через социальную сеть.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Имя учетной записи в социальной сети</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="website" use="optional" type="tokenType"> 
            <xsd:annotation> 
                <xsd:documentation>Адрес веб-сайта.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Веб-сайт</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="issuer" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>URL источника.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Источник</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="realmNameAttribute" use="optional" default="iss" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Значение требования используется в качестве области субъекта.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Затребовать как область</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupNameAttribute" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Значение требования используется как членство в группах пользователей.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Затребовать как группу</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userUniqueIdAttribute" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Значение требования используется в качестве uniqueId субъекта.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Затребовать как уникальный ИД субъекта</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clockSkew" use="optional" default="300000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>Максимальная разница во времени между моментом выпуска ключа и моментом его использования.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Разница во времени</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="signatureAlgorithm" use="optional" default="RS256" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Алгоритм, используемый для подписи маркера или ключа.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Алгоритм подписи</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenEndpointAuthMethod" use="optional" default="client_secret_post"> 
            <xsd:annotation> 
                <xsd:documentation>Требуемый способ идентификации.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Способ идентификации</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="client_secret_basic"> 
                                <xsd:annotation> 
                                    <xsd:documentation>client_secret_basic</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="client_secret_post"> 
                                <xsd:annotation> 
                                    <xsd:documentation>client_secret_post</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="redirectToRPHostAndPort" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Задает протокол, хост и номер порта обратного вызова. Например, https://myhost:8020.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Хост и номер порта обратного вызова</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="hostNameVerificationEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Указывает, следует ли включить проверку имени хоста, когда клиент связывается с провайдером. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Проверка имени хоста включена</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="responseType" use="optional" default="code"> 
            <xsd:annotation> 
                <xsd:documentation>Тип ответа OAuth.</xsd:documentation> 
