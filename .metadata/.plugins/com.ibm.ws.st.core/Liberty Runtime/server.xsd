        <xsd:attribute name="authDataRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Идентификационные данные по умолчанию для спецификации активации.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ссылка на идентификационные данные</ext:label> 
                    <ext:reference>authData</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxEndpoints" use="optional" default="500"> 
            <xsd:annotation> 
                <xsd:documentation>Максимальное число конечных точек, в которые осуществляется диспетчеризация.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Максимальное число конечных точек</ext:label> 
