    <xsd:complexType name="com.ibm.ws.javaee.dd.ejbbnd.Session"> 
        <xsd:annotation> 
            <xsd:documentation>Свойства сеансового EJB в приложении EJB.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Сеанс</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="ejb-ref" type="com.ibm.ws.javaee.dd.commonbnd.EJBRef-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает привязку для ссылки на объект EJB.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Ссылки на объекты EJB</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="resource-ref" type="com.ibm.ws.javaee.dd.commonbnd.ResourceRef-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает привязку для ссылки на ресурс.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Ссылки на ресурсы</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="resource-env-ref" type="com.ibm.ws.javaee.dd.commonbnd.ResourceEnvRef-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает привязку для ссылки на среду ресурсов.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Ссылки на среды ресурсов</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="message-destination-ref" type="com.ibm.ws.javaee.dd.commonbnd.MessageDestinationRef-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает привязку для ссылки на целевой объект сообщения.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Целевые объекты сообщений</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="data-source" type="com.ibm.ws.javaee.dd.commonbnd.DataSource-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает привязку для источника данных.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Источники данных</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="env-entry" type="com.ibm.ws.javaee.dd.commonbnd.EnvEntry-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает привязку для записи среды.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Записи среды</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="interface" type="com.ibm.ws.javaee.dd.ejbbnd.Interface-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает интерфейс сеанса.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Интерфейс</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="name" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Имя объекта EJB.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Имя</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="simple-binding-name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Задает имя простой привязки для сеансового EJB.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Имя простой привязки</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="component-id" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>ИД компонента для сеансового EJB.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ИД компонента</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="remote-home-binding-name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Имя привязки удаленной начальной страницы для сеансового EJB.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Имя привязки удаленной начальной страницы</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="local-home-binding-name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Имя привязки локальной начальной страницы для сеансового EJB.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Имя привязки локальной начальной страницы</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
