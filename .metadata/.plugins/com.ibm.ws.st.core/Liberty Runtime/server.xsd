    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress"> 
        <xsd:annotation> 
            <xsd:documentation>Задает адрес TCP/IP удаленного хоста клиентского приложения, отправившего запрос HTTP, например &lt;remoteAddress id="sample" ip="100.100.100.99" matchType="greaterThan"/&gt;.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Удаленный адрес</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="matchType" use="optional" default="contains"> 
            <xsd:annotation> 
                <xsd:documentation>Задает тип сравнения.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Тип сравнения</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="equals"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Равно</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="contains"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Содержит</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="notContain"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Не содержит</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="greaterThan"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Больше, чем</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="lessThan"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Меньше, чем</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="ip" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Задает адрес TCP/IP удаленного хоста. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>IP-адрес</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.authentication.filter.remoteAddress"> 
