                <ext:action elemPrefix="properties." idAttr="${id}" alternateIdAttr="${location}" type="generateSchema"/> 
                <ext:label>Адаптер ресурсов</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader"> 
                <xsd:annotation> 
                    <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Загрузчик классов</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"> 
            </xsd:element> 
            <xsd:element name="customize" type="com.ibm.ws.jca.resourceAdapter.customize"> 
            </xsd:element> 
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"> 
            </xsd:element> 
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"> 
            </xsd:element> 
            <xsd:any processContents="skip" minOccurs="0" maxOccurs="1"/> 
        </xsd:choice> 
        <xsd:attribute name="location" use="required" type="location"> 
            <xsd:annotation> 
                <xsd:documentation>Определяет путь к файлу RAR для установки.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Путь к файлу RAR</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="autoStart" use="optional" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Указывает, следует ли запускать адаптер ресурсов автоматически после его развертывания или отложенно при выполнении операции вставки или поиска ресурса. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Запускать автоматически</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useJandex" use="optional" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Включает использование индексных файлов Jandex при их наличии в приложении</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Использовать Jandex</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="classloaderRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Загрузчик классов</ext:label> 
                    <ext:reference>classloader</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
