    <xsd:complexType name="com.ibm.ws.security.oidcs.scopeToClaimMap"> 
        <xsd:annotation> 
            <xsd:documentation>Укажите требования для области.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Карта связей области с требованиями</ext:label> 
                <ext:extraProperties/> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="property" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.security.oidcs.property-factory"> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="profile" use="optional" default="name, family_name, given_name, middle_name, nickname, preferred_username, profile, picture, website, gender, birthdate, zoneinfo, locale, updated_at" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Введите список требований, связанных с областью profile, разделяя их запятыми. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Область profile</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="email" use="optional" default="email, email_verified" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Введите список требований, связанных с областью email, разделяя их запятыми. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Область email</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="address" use="optional" default="address" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Введите список требований, связанных с областью address, разделяя их запятыми.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Область address</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="phone" use="optional" default="phone_number, phone_number_verified" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Введите список требований, связанных с областью phone, разделяя их запятыми.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Область phone</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>Укажите требования для области.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Карта связей области с требованиями</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oidcs.scopeToClaimMap-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.oidcs.scopeToClaimMap"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation> 
                        <xsd:appinfo> 
