            <xsd:appinfo> 
                <ext:label>Запись контекста сеансов JAAS</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
