                <xsd:appinfo> 
                    <ext:label>Значение свойства уровня приложения</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.appconfig.appProperties.property-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.appconfig.appProperties.property"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ИД</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
